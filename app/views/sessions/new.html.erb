
  <div class="welcome-page">
    <div class="col-md-4 pull-right">
      <h1 style="pull-left">Log in</h1>

      <%= form_tag sessions_path do %>
      <p>
          <%= label_tag :email %><br/>
          <%= text_field_tag :email, params[:email], :style => "color : black" %>
      </p>
      <p>
          <%= label_tag :password %><br/>
          <%= password_field_tag :password %>
      </p>

      <p class="button", ><%= submit_tag "Log in", class: "button" %></p>
      <% end %>
    </div>

  <div class="col-md-8">
    <section class="center-welcome">
      <h1>Welcome</h1>
      <h2>Blocipedia: SaaS Wiki Collaboration Tool</h2>
    </section>
  </div>
</div>
